{
  "extends": ["eslint:recommended", "next/core-web-vitals"],
  "rules": {
    "no-console": "warn",
    "no-duplicate-imports": "error",
    "no-unused-vars": "error",
    "no-var": "error",
    "no-empty": "error",
    "eqeqeq": "error",
    "@next/next/no-img-element": "error"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
